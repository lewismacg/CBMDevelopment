@{
	Layout = "_Layout";
	ViewData["Title"] = "Previous work and ongoing projects";
}
@section AdditionalMeta
{
	<meta name="description" content="CBM Developments - Previous Work. Have a peek at our previous projects and what we have coming up in the pipeline." />
	<meta name="robots" content="index, follow">
	<meta property="og:title" content="CBM Developments | Previous Work" />
	<meta property="og:description" content="CBM Developments - Previous work. Have a peek at our previous projects and what we have coming up in the pipeline." />
	<meta property="og:locale" content="en_UK" />
	<meta property="og:type" content="website" />
	<meta property="og:site_name" content="CBM Developments" />
}
<div class="row">
	<div class="col-sm-12">
		<div class="header-wrapper header-image previous-work">
			<h1>
				Previous and ongoing projects
				<br />
				by CBM Developments.
			</h1>
		</div>
	</div>
</div>
<div class="row">
	<div class="col-sm-12">
		<div class="sub-nav">
			<div class="sub-nav-link">
				<a href="#" id="subnav-one">4 Bedroom Detached Property</a>
			</div>
			<div class="sub-nav-link">
				<a href="#" id="subnav-two">Further Projects</a>
			</div>
		</div>
	</div>
</div>
<div class="container">
	<div id="project1">
		<div class="row content-box-shadow content-box-padding">
			<div class="col-md-5">
				<div id="detached-property-carousel" class="carousel slide" data-ride="carousel">
					<ol class="carousel-indicators">
						<li data-target="#detached-property-carousel" data-slide-to="0" class="active"></li>
						<li data-target="#detached-property-carousel" data-slide-to="1"></li>
						<li data-target="#detached-property-carousel" data-slide-to="2"></li>
						<li data-target="#detached-property-carousel" data-slide-to="3"></li>
						<li data-target="#detached-property-carousel" data-slide-to="4"></li>
						<li data-target="#detached-property-carousel" data-slide-to="5"></li>
						<li data-target="#detached-property-carousel" data-slide-to="6"></li>
						<li data-target="#detached-property-carousel" data-slide-to="7"></li>
						<li data-target="#detached-property-carousel" data-slide-to="8"></li>
						<li data-target="#detached-property-carousel" data-slide-to="9"></li>
					</ol>

					<div class="carousel-inner slideshow-work">
						<div class="carousel-item active">
							<img alt="Detached Property first slide" class="d-block" src="~/img/detached_property/project_1a.jpeg" />
						</div>
						<div class="carousel-item">
							<img alt="Detached Property second slide" class="d-block" src="~/img/detached_property/project_1b.jpeg" />
						</div>
						<div class="carousel-item">
							<img alt="Detached Property third slide" class="d-block" src="~/img/detached_property/project_1c.jpeg" />
						</div>
						<div class="carousel-item">
							<img alt="Detached Property fourth slide" class="d-block" src="~/img/detached_property/project_1d.jpeg" />
						</div>
						<div class="carousel-item">
							<img alt="Detached Property fifth slide" class="d-block" src="~/img/detached_property/project_1e.jpeg" />
						</div>
						<div class="carousel-item">
							<img alt="Detached Property sixth slide" class="d-block" src="~/img/detached_property/project_1f.jpeg" />
						</div>
						<div class="carousel-item">
							<img alt="Detached Property seventh slide" class="d-block" src="~/img/detached_property/project_1g.jpeg" />
						</div>
						<div class="carousel-item">
							<img alt="Detached Property eighth slide" class="d-block img-fluid" src="~/img/detached_property/project_1h.jpeg" />
						</div>
						<div class="carousel-item">
							<img alt="Detached Property ninth slide" class="d-block img-fluid" src="~/img/detached_property/project_1i.jpeg" />
						</div>
						<div class="carousel-item">
							<img alt="Detached Property tenth slide" class="d-block img-fluid" src="~/img/detached_property/project_1j.jpeg" />
						</div>
					</div>
					<a class="carousel-control-prev" href="#detached-property-carousel" role="button" data-slide="prev">
						<span class="carousel-control-prev-icon" aria-hidden="true"></span>
						<span class="sr-only">Previous</span>
					</a>
					<a class="carousel-control-next" href="#detached-property-carousel" role="button" data-slide="next">
						<span class="carousel-control-next-icon" aria-hidden="true"></span>
						<span class="sr-only">Next</span>
					</a>
				</div>
			</div>
			<div class="col-md-7">
				<div class="row pt-3">
					<div class="col-md-12"><h2 class="emphasis gold">4 Bedroom Detached Property, Ketley</h2></div>
				</div>
				<hr />
				<div class="row">
					<div class="col-md-12">
						<p>
							Working closely with friends at <a class="gold" href="https://www.oliverhardyhomes.co.uk/" rel="nofollow noopener noreferrer">Oliver Hardy Homes</a>,
							our first development project involved learning the ropes alongside experienced developers located in the Shropshire area.
						</p>
						<br />
						<p>
							With the homes complete now, CBM Developments is ready to move on to new ventures. Keep your eyes peeled here
							for any new or upcoming developments.
						</p>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div id="project2" style="display: none">
		<div class="row content-box-shadow content-box-padding">
			<div class="col-md-2 offset-md-1 align-self-center text-center pt-3">
				<img alt="Seeking investments" class="img-fluid" style="max-height: 300px" src="~/img/cbm_developments_logo_invert.png" />
			</div>
			<div class="offset-md-1 col-md-8">
				<div class="row pt-3">
					<div class="col-12"><h2 class="emphasis gold">Awaiting Investment</h2></div>
				</div>
				<hr />
				<div class="row">
					<div class="col-12">
						<p>CBM Developments is searching for <strong>investors</strong> and <strong>landowners</strong> alike for development opportunities.</p>
						<p>Have funds sitting in the bank earning very little interest? Get in touch with us and we can talk you through how you can earn a tidy sum on a <strong>minimal investment</strong>.</p>
						<p>Have an area of land and looking to sell, or unsure about the processes required to get started? We can talk you through the process and help you with any questions you may have.</p>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

@section Scripts {
	<script src="~/js/switch-section.min.js"></script>
}